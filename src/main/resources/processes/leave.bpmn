    </sequenceFlow>
    <endEvent id="endevent3" name="结束"></endEvent>
    <sequenceFlow id="flow5" sourceRef="exclusivegateway1" targetRef="endevent3">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${days<=1}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="usertask2" name="经理审核" activiti:candidateUsers="${ManagerCandidateList}"></userTask>
